{
  "name": "broo",
  "displayName": "Broo",
  "description": "Syntax highlighting for .broo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "broo",
      "aliases": ["Broo", "broo"],
      "extensions": [".broo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "broo",
      "scopeName": "source.broo",
      "path": "./syntaxes/broo.tmLanguage.json"
    }]
  }
}
